<h3><%= t('map.maps')%></h3>
<table class="table table-striped"><tr>
  <th></th>
  <th><%= t('map.code')%></th>
	<th><%= t('map.datetime')%></th>
</tr>
<% @maps.each do |m| %>
  <tr>
    <td><%if !m.image.nil?%><%= link_to image_tag("maps/" + m.image ,width: "150"), m, :target => "_blank" %><%end%></td>
    <td><%= link_to m.code, m, :target =>"_blank" %></td>
		<td><%= m.datetime %></td>
</tr>
<%end%>
</table>
